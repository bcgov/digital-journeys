<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:camunda="http://camunda.org/schema/1.0/bpmn" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_0lk9z5q" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.16.0" modeler:executionPlatform="Camunda Platform" modeler:executionPlatformVersion="7.17.0"><bpmn:process id="maternity-parental-leave-and-allowance" name="maternity-parental-leave-and-allowance" isExecutable="true" camunda:versionTag="1"><bpmn:userTask id="Activity_0qfrc2u" name="${formName} for ${name} - ${applicationStatus}" camunda:assignee="${managerEmail_2}_idir" camunda:candidateUsers="${email}_idir"><bpmn:extensionElements><camunda:taskListener event="complete"><camunda:script scriptFormat="javascript">var action =
                        task.getExecution().getVariable('action');
                        task.getExecution().setVariable('applicationStatus', action);
                        task.getExecution().setVariable('managersSignatureTimestamp', new
                        Date().toISOString());</camunda:script></camunda:taskListener><camunda:taskListener class="org.camunda.bpm.extension.hooks.listeners.AddFormPermissionListener" event="create" id="AddFormPermission"><camunda:field name="permissions"><camunda:expression>["read", "write"]</camunda:expression></camunda:field><camunda:field name="user"><camunda:expression>${managerEmail_2}</camunda:expression></camunda:field></camunda:taskListener><camunda:taskListener class="org.camunda.bpm.extension.hooks.listeners.UserGroupAssignmentListener" event="create" id="user-group-assigned"><camunda:field name="groupPath"><camunda:string>/formsflow/manager</camunda:string></camunda:field></camunda:taskListener></bpmn:extensionElements><bpmn:incoming>Flow_19jo2x6</bpmn:incoming><bpmn:incoming>Flow_0t3cb8j</bpmn:incoming><bpmn:incoming>Flow_1r6ctdu</bpmn:incoming><bpmn:incoming>Flow_0x1ldi7</bpmn:incoming><bpmn:incoming>Flow_0gx4c7k</bpmn:incoming><bpmn:incoming>Flow_1uo26n8</bpmn:incoming><bpmn:outgoing>Flow_08x5h59</bpmn:outgoing></bpmn:userTask><bpmn:endEvent id="Event_0mogxwp" name="End "><bpmn:incoming>Flow_0r6mubm</bpmn:incoming></bpmn:endEvent><bpmn:sequenceFlow id="Flow_08x5h59" sourceRef="Activity_0qfrc2u" targetRef="Task_1hko8r7"><bpmn:extensionElements /></bpmn:sequenceFlow><bpmn:exclusiveGateway id="Gateway_09ic8nm" name="Should end?"><bpmn:incoming>Flow_1wf89p6</bpmn:incoming><bpmn:outgoing>Flow_0lmv6hd</bpmn:outgoing><bpmn:outgoing>Flow_0750fx8</bpmn:outgoing></bpmn:exclusiveGateway><bpmn:sequenceFlow id="Flow_0lmv6hd" name="false" sourceRef="Gateway_09ic8nm" targetRef="Activity_teleworkDeleteFinalSubmission"><bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${false}</bpmn:conditionExpression></bpmn:sequenceFlow><bpmn:serviceTask id="maternity-form-send-confirmation-email" name="Send Confirmation Email to Submitter" camunda:class="org.camunda.bpm.extension.hooks.listeners.TaskAssignmentListener"><bpmn:extensionElements><camunda:inputOutput><camunda:inputParameter name="subject">${formName} Received</camunda:inputParameter><camunda:inputParameter name="body">&lt;p&gt;This is to confirm that we have
                        received your application for maternity/parental leave. Your application has
                        been forwarded to your supervisor for review and approval. Once your
                        supervisor approves your leave, we will notify you via email. You can also
                        check the status of your application in the "Submitted Forms" tab on &lt;a
                        href="https://digital-journeys.apps.silver.devops.gov.bc.ca/form"
                        rel="noopener noreferrer nofollow" target="_blank"&gt;Digital
                        Journeys&lt;/a&gt;.
                        &lt;/p&gt;
&lt;p&gt;
Your supervisor can make limited changes to your form on your behalf. They can update your leave start date, length of leave and parental leave allowance dates. They can access your form on "Review and Process" tab in &lt;a href="https://digital-journeys.apps.silver.devops.gov.bc.ca/form" rel="noopener noreferrer nofollow" target="_blank"&gt;Digital Journeys&lt;/a&gt;.
&lt;/p&gt;</camunda:inputParameter><camunda:inputParameter name="recipientEmails">${email}</camunda:inputParameter><camunda:inputParameter name="attachSubmission">false</camunda:inputParameter></camunda:inputOutput></bpmn:extensionElements><bpmn:incoming>Flow_1vmy8a8</bpmn:incoming><bpmn:outgoing>Flow_138be1z</bpmn:outgoing></bpmn:serviceTask><bpmn:sequenceFlow id="Flow_19jo2x6" sourceRef="maternity-form-send-email-to-supervisor" targetRef="Activity_0qfrc2u"><bpmn:extensionElements /></bpmn:sequenceFlow><bpmn:task id="Task_1hko8r7" name="Update Application Status"><bpmn:extensionElements><camunda:executionListener class="org.camunda.bpm.extension.hooks.listeners.ApplicationStateListener" event="end" /><camunda:executionListener class="org.camunda.bpm.extension.hooks.listeners.BPMFormDataPipelineListener" event="start"><camunda:field name="fields"><camunda:expression>["applicationId","applicationStatus",
                            "managersSignatureTimestamp"]</camunda:expression></camunda:field></camunda:executionListener><camunda:executionListener class="org.camunda.bpm.extension.hooks.listeners.FormBPMDataPipelineListener" event="start" /></bpmn:extensionElements><bpmn:incoming>Flow_08x5h59</bpmn:incoming><bpmn:outgoing>Flow_0b5yqyg</bpmn:outgoing></bpmn:task><bpmn:startEvent id="StartEvent_1" name="Submitted form"><bpmn:outgoing>Flow_1sb50cc</bpmn:outgoing></bpmn:startEvent><bpmn:sequenceFlow id="Flow_1sb50cc" sourceRef="StartEvent_1" targetRef="Activity_1wx7jfk"><bpmn:extensionElements><camunda:executionListener class="org.camunda.bpm.extension.hooks.listeners.BPMFormDataPipelineListener" event="take"><camunda:field name="fields"><camunda:expression>["applicationId","applicationStatus"]</camunda:expression></camunda:field></camunda:executionListener><camunda:executionListener class="org.camunda.bpm.extension.hooks.listeners.ApplicationStateListener" event="take" /><camunda:executionListener class="org.camunda.bpm.extension.hooks.listeners.FormBPMDataPipelineListener" event="take" /><camunda:executionListener event="take"><camunda:script scriptFormat="javascript">var initialStatus = execution.getVariable('initialStatus');
execution.setVariable('applicationStatus', initialStatus);</camunda:script></camunda:executionListener></bpmn:extensionElements></bpmn:sequenceFlow><bpmn:task id="Activity_1wx7jfk" name="Update Application Status"><bpmn:extensionElements><camunda:executionListener class="org.camunda.bpm.extension.hooks.listeners.BPMFormDataPipelineListener" event="end" /><camunda:executionListener class="org.camunda.bpm.extension.hooks.listeners.ApplicationStateListener" event="start" /></bpmn:extensionElements><bpmn:incoming>Flow_1sb50cc</bpmn:incoming><bpmn:outgoing>Flow_0iihtti</bpmn:outgoing></bpmn:task><bpmn:sequenceFlow id="Flow_0iihtti" sourceRef="Activity_1wx7jfk" targetRef="Gateway_1658szm" /><bpmn:sequenceFlow id="Flow_138be1z" sourceRef="maternity-form-send-confirmation-email" targetRef="maternity-form-send-email-to-supervisor" /><bpmn:serviceTask id="maternity-form-send-email-to-supervisor" name="Send email to supervisor for review" camunda:class="org.camunda.bpm.extension.hooks.listeners.TaskAssignmentListener"><bpmn:extensionElements><camunda:inputOutput><camunda:inputParameter name="subject">${name}'s  ${formName} Received For Approval</camunda:inputParameter><camunda:inputParameter name="body">This is to inform you that a new
                        maternity/parental leave application has been submitted by ${name} and is
                        ready for your review and approval. The form can be found in the
                        "Review &amp; Process" tab on &lt;a
                        href="https://digital-journeys.apps.silver.devops.gov.bc.ca/task"
                        rel="noopener noreferrer nofollow" target="_blank"&gt;Digital
                        Journeys&lt;/a&gt;.

                        &lt;p&gt;
                        The requested start date for the leave is ${hiddenStartDate}. We recommend
                        that you approve this form as soon as possible, but no later than 4
                        weeks before the start date. This will allow timely processing of the
                        form and prevent payroll errors.
                        &lt;/p&gt;
&lt;p&gt;
As the supervisor you can make limited changes to the form on behalf of the employee. You can update the leave start date, length of leave and parental leave allowance dates. You can access the form in the "Review &amp; Process" tab on &lt;a
                        href="https://digital-journeys.apps.silver.devops.gov.bc.ca/task"
                        rel="noopener noreferrer nofollow" target="_blank"&gt;Digital
                        Journeys&lt;/a&gt;.
&lt;/p&gt;
&lt;p&gt;
    &lt;b&gt;NOTE:&lt;/b&gt; If the employee is taking &lt;b&gt;only parental leave&lt;/b&gt;, a leave can be approved with a future date of birth, however, it is the &lt;b&gt;responsibility of the supervisor and the employee to ensure parental leave is started only after the child is born&lt;/b&gt;. If there is any change in the date of the birth or leave start date, the employee or supervisor can edit them on 
    &lt;a href="https://digital-journeys.apps.silver.devops.gov.bc.ca/form" rel="noopener noreferrer nofollow" target="_blank"&gt;Digital Journeys&lt;/a&gt; and resubmit. Not doing so can lead to payroll errors.
&lt;/p&gt;</camunda:inputParameter><camunda:inputParameter name="recipientEmails">${managerEmail_2}</camunda:inputParameter><camunda:inputParameter name="attachSubmission">false</camunda:inputParameter></camunda:inputOutput></bpmn:extensionElements><bpmn:incoming>Flow_138be1z</bpmn:incoming><bpmn:incoming>Flow_08pkhho</bpmn:incoming><bpmn:outgoing>Flow_19jo2x6</bpmn:outgoing></bpmn:serviceTask><bpmn:serviceTask id="Activity_0t3fkjy" name="Create CRM Ticket" camunda:class="org.camunda.bpm.extension.hooks.listeners.CrmDelegate"><bpmn:incoming>Flow_051yvpj</bpmn:incoming><bpmn:outgoing>Flow_1150tbp</bpmn:outgoing></bpmn:serviceTask><bpmn:sequenceFlow id="Flow_1150tbp" sourceRef="Activity_0t3fkjy" targetRef="Activity_0wm3rzj" /><bpmn:exclusiveGateway id="Gateway_0a7n9h5" default="Flow_1414hfz"><bpmn:incoming>Flow_0b5yqyg</bpmn:incoming><bpmn:outgoing>Flow_051yvpj</bpmn:outgoing><bpmn:outgoing>Flow_1414hfz</bpmn:outgoing></bpmn:exclusiveGateway><bpmn:sequenceFlow id="Flow_051yvpj" name="on manager validation (approval/denial)" sourceRef="Gateway_0a7n9h5" targetRef="Activity_0t3fkjy"><bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${action == managerApprovedStatus or action == managerDeniedStatus}</bpmn:conditionExpression></bpmn:sequenceFlow><bpmn:sequenceFlow id="Flow_1414hfz" name="else" sourceRef="Gateway_0a7n9h5" targetRef="Activity_0wm3rzj" /><bpmn:sequenceFlow id="Flow_1ubprla" sourceRef="Event_18it1xb" targetRef="Gateway_194668j" /><bpmn:task id="Activity_0wm3rzj" name="Send data to forms"><bpmn:extensionElements><camunda:executionListener class="org.camunda.bpm.extension.hooks.listeners.BPMFormDataPipelineListener" event="start"><camunda:field name="fields"><camunda:expression>["applicationId","applicationStatus",
                            "managersSignatureTimestamp", "crmId", "crmLookupName"]</camunda:expression></camunda:field></camunda:executionListener></bpmn:extensionElements><bpmn:incoming>Flow_1414hfz</bpmn:incoming><bpmn:incoming>Flow_1150tbp</bpmn:incoming><bpmn:outgoing>Flow_1wf89p6</bpmn:outgoing></bpmn:task><bpmn:sequenceFlow id="Flow_1wf89p6" sourceRef="Activity_0wm3rzj" targetRef="Gateway_09ic8nm" /><bpmn:sequenceFlow id="Flow_0b5yqyg" sourceRef="Task_1hko8r7" targetRef="Gateway_0a7n9h5" /><bpmn:task id="Activity_0bk156a" name="Update reminderEmailDate data"><bpmn:extensionElements><camunda:executionListener event="start"><camunda:script scriptFormat="javascript">execution.setVariable('reminderEmailDate', new Date(new Date().getFullYear() + 25, 0, 1).toISOString().split('.')[0] + 'Z');
</camunda:script></camunda:executionListener></bpmn:extensionElements><bpmn:incoming>Flow_0mv3lvc</bpmn:incoming><bpmn:incoming>Flow_0yiy4dc</bpmn:incoming><bpmn:outgoing>Flow_0y87ojg</bpmn:outgoing></bpmn:task><bpmn:sequenceFlow id="Flow_0y87ojg" sourceRef="Activity_0bk156a" targetRef="Activity_10t4yke" /><bpmn:task id="Activity_10t4yke" name="Send data to formio"><bpmn:extensionElements><camunda:executionListener class="org.camunda.bpm.extension.hooks.listeners.BPMFormDataPipelineListener" event="start"><camunda:field name="fields"><camunda:expression>["applicationId","applicationStatus", "managersSignatureTimestamp", "crmId", "crmLookupName","reminderEmailDate"]</camunda:expression></camunda:field></camunda:executionListener></bpmn:extensionElements><bpmn:incoming>Flow_0y87ojg</bpmn:incoming><bpmn:outgoing>Flow_0t3cb8j</bpmn:outgoing></bpmn:task><bpmn:sequenceFlow id="Flow_0t3cb8j" sourceRef="Activity_10t4yke" targetRef="Activity_0qfrc2u" /><bpmn:serviceTask id="Activity_1de7ree" name="Send reminder email" camunda:class="org.camunda.bpm.extension.hooks.listeners.TaskAssignmentListener"><bpmn:extensionElements><camunda:inputOutput><camunda:inputParameter name="subject">Reminder to notify return to work 4 weeks before Maternity/Parental Leave</camunda:inputParameter><camunda:inputParameter name="attachSubmission">false</camunda:inputParameter><camunda:inputParameter name="recipientEmails">${reminderEmailRecipients}</camunda:inputParameter><camunda:inputParameter name="body">&lt;p&gt;
  This is to inform you that you must notify your supervisor about your intent
  to return to work at least 4 weeks before your intended return date. To notify
  your supervisor complete this
  &lt;a
    class="VXqH9lW2iuelPh86QVEd"
    href="https://www2.gov.bc.ca/assets/download/E7ED327147F843678802958B9310F218"
    rel="noopener noreferrer nofollow"
    target="_blank"
    &gt;letter template&lt;/a
  &gt;
  and ask your supervisor to submit an
  &lt;a
    class="VXqH9lW2iuelPh86QVEd"
    href="https://sfs7.gov.bc.ca/affwebservices/public/saml2sso?SPID=urn:ca:bc:gov:customerportal:prod"
    rel="noopener noreferrer nofollow"
    target="_blank"
    &gt;AskMyHR&lt;/a
  &gt;
  service request for "My Team or Organization" under the following category
  "Leave &amp; Time Off &gt; Maternity, Parental, Adoption" with the letter attached.
&lt;/p&gt;
&lt;p&gt;You must also inform your supervisor if:&lt;/p&gt;
&lt;ol&gt;
  &lt;li&gt;
    You're requesting an extension to your leave, and you have not taken the
    maximum parental leave and extended childcare leave (combined maximum of 78
    weeks total).
  &lt;/li&gt;
  &lt;li&gt;
    You're wanting to return earlier than the date you indicated on your digital
    application; or
  &lt;/li&gt;
  &lt;li&gt;You're resigning and not returning to work.&lt;/li&gt;
&lt;/ol&gt;
&lt;p&gt;
  Ask your supervisor to submit an AskMyHR service request for "My Team or
  Organization" under the following category "Leave &amp; Time Off &gt; Maternity,
  Parental, Adoption" advising that you're returning early, extending your leave
  or resigning. AskMyHR will process the request and confirm when the request is
  completed.
&lt;/p&gt;
&lt;p&gt;
  There are things to consider if you're extending your leave, returning to work
  or resigning.
&lt;/p&gt;
&lt;ul&gt;
  &lt;li&gt;
    If you're extending your parental leave and not receiving leave allowance,
    there won't be any pay to deduct your premiums for any of the optional life
    insurance plans. If you would like to pay the premiums to maintain any
    optional life insurance, complete an
    &lt;a
      class="VXqH9lW2iuelPh86QVEd"
      href="https://www2.gov.bc.ca/assets/gov/careers/all-employees/leave-and-time-off/continue_benefits_esa_loa_without_pay.pdf"
      rel="noopener noreferrer nofollow"
      target="_blank"
      &gt;Option to Continue Benefits While on Employment Standards Act Leave of
      Absence Without Pay form&lt;/a
    &gt;. Submit it to your supervisor to submit to AskMyHR. You will receive a
    statement of cost.
  &lt;/li&gt;
  &lt;li&gt;
    If you're taking extended childcare leave, your benefits will be cancelled
    at the end of the month in which you start extended childcare leave. You can
    pay the premiums to maintain any of your benefits by completing the
    &lt;a
      class="VXqH9lW2iuelPh86QVEd"
      href="https://www2.gov.bc.ca/assets/gov/careers/all-employees/leave-and-time-off/continue_benefits_loa_without_pay_layoff.pdf"
      rel="noopener noreferrer nofollow"
      target="_blank"
      &gt;Option to Continue Benefits While on Leave of Absence Without Pay or
      Layoff&lt;/a
    &gt;. Submit it to your supervisor to submit to AskMyHR. You will receive a
    statement of cost.
  &lt;/li&gt;
  &lt;li&gt;
    If you have deferred your allowance and you wish to claim it when you
    return, submit a copy of "My Latest Claim" (which can be found by logging
    into your account with EI) and submitting it to
    &lt;a
      class="VXqH9lW2iuelPh86QVEd"
      href="https://sfs7.gov.bc.ca/affwebservices/public/saml2sso?SPID=urn:ca:bc:gov:customerportal:prod"
      rel="noopener noreferrer nofollow"
      target="_blank"
      &gt;AskMyHR&lt;/a
    &gt;
    service request for "Myself" under the following category "Leave &amp; Time Off
    &gt; Maternity, Parental, Adoption".
  &lt;/li&gt;
  &lt;li&gt;
    If you cancelled your benefits while you were on leave and want to reinstate
    them upon your return to work, you must complete the applicable application
    form and submit it to AskMyHR under the following category "Myself &gt;
    Benefits &gt; Bargaining Unit or Excluded Employees"
    &lt;ul&gt;
      &lt;li&gt;
        &lt;a
          class="VXqH9lW2iuelPh86QVEd"
          href="https://www2.gov.bc.ca/assets/gov/careers/all-employees/pay-and-benefits/benefits-for-bargaining-unit-employees/bargaining_unit_benefits_enrolment_change.pdf"
          rel="noopener noreferrer nofollow"
          target="_blank"
          &gt;Bargaining Unit Benefits Program Enrolment/Change Form&lt;/a
        &gt;
      &lt;/li&gt;
      &lt;li&gt;
        &lt;a
          class="VXqH9lW2iuelPh86QVEd"
          href="https://www2.gov.bc.ca/assets/gov/careers/all-employees/pay-and-benefits/benefits-for-excluded-employees/flexible_benefits_enrolment_change.pdf"
          rel="noopener noreferrer nofollow"
          target="_blank"
          &gt;Flexible Benefits Program Enrolment/Change Form&lt;/a
        &gt;
      &lt;/li&gt;
    &lt;/ul&gt;
  &lt;/li&gt;
  &lt;li&gt;
    If you suspended any
    &lt;a
      class="VXqH9lW2iuelPh86QVEd"
      href="https://www2.gov.bc.ca/gov/content/careers-myhr/all-employees/leave-time-off/cancel-deductions"
      rel="noopener noreferrer nofollow"
      target="_blank"
      &gt;deductions&lt;/a
    &gt;
    during your leave, such as bus pass, charitable donations, deferred salary
    leave, etc., contact AskMyHR to reinstate them.
  &lt;/li&gt;
  &lt;li&gt;
    Your pension contributions are based on the leave allowance you received
    while on leave. You can purchase the remaining service for your leave within
    five years of the end of your leave period or before termination of
    employment, whichever occurs first. For more information, refer to the:
    &lt;ul&gt;
      &lt;li&gt;
        &lt;a
          class="VXqH9lW2iuelPh86QVEd"
          href="https://www2.gov.bc.ca/gov/content/careers-myhr/all-employees/pay-benefits/benefits/pension/purchase-service"
          rel="noopener noreferrer nofollow"
          target="_blank"
          &gt;Purchasing Service&lt;/a
        &gt;
      &lt;/li&gt;
      &lt;li&gt;
        &lt;a
          class="VXqH9lW2iuelPh86QVEd"
          href="https://pspp.pensionsbc.ca/"
          rel="noopener noreferrer nofollow"
          target="_blank"
          &gt;Public Service Pension Plan website&lt;/a
        &gt;
      &lt;/li&gt;
    &lt;/ul&gt;
  &lt;/li&gt;
  &lt;li&gt;
    If you're resigning to care for your dependent, you may be eligible for
    &lt;a
      class="VXqH9lW2iuelPh86QVEd"
      href="https://www2.gov.bc.ca/gov/content/careers-myhr/all-employees/leaving-the-public-service/bridging-service"
      rel="noopener noreferrer nofollow"
      target="_blank"
      &gt;Bridging of Service&lt;/a
    &gt;.
    &lt;ul&gt;
      &lt;li&gt;
        You must notify your supervisor and note that you're Bridging Service in
        your resignation letter.
      &lt;/li&gt;
      &lt;li&gt;Bridging of service cannot be claimed retroactively.&lt;/li&gt;
    &lt;/ul&gt;
  &lt;/li&gt;
&lt;/ul&gt;</camunda:inputParameter></camunda:inputOutput></bpmn:extensionElements><bpmn:incoming>Flow_1e1gn2c</bpmn:incoming><bpmn:outgoing>Flow_0mv3lvc</bpmn:outgoing></bpmn:serviceTask><bpmn:sequenceFlow id="Flow_0mv3lvc" sourceRef="Activity_1de7ree" targetRef="Activity_0bk156a" /><bpmn:serviceTask id="Activity_1k8g7vg" name="Send leave&#10;&#10; approval email" camunda:class="org.camunda.bpm.extension.hooks.listeners.TaskAssignmentListener"><bpmn:extensionElements><camunda:inputOutput><camunda:inputParameter name="body">${email_body}</camunda:inputParameter><camunda:inputParameter name="subject">${email_subject}</camunda:inputParameter><camunda:inputParameter name="recipientEmails">${email}</camunda:inputParameter><camunda:inputParameter name="attachSubmission">true</camunda:inputParameter><camunda:inputParameter name="attachSubmissionName">Maternity-and-parental-leave-form</camunda:inputParameter></camunda:inputOutput></bpmn:extensionElements><bpmn:incoming>Flow_13z2gbm</bpmn:incoming><bpmn:outgoing>Flow_1nkaatw</bpmn:outgoing></bpmn:serviceTask><bpmn:serviceTask id="Activity_1f42pci" name="Send email to supervisor about change (with PDF)" camunda:class="org.camunda.bpm.extension.hooks.listeners.TaskAssignmentListener"><bpmn:extensionElements><camunda:inputOutput><camunda:inputParameter name="recipientEmails">${managerEmail_2}</camunda:inputParameter><camunda:inputParameter name="subject">${name}'s ${formName} Edited Successfully</camunda:inputParameter><camunda:inputParameter name="attachSubmission">true</camunda:inputParameter><camunda:inputParameter name="body">&lt;p&gt;This is to inform you that ${name}'s application for maternity/parental leave has been edited successfully. You can find a pdf of the application attached in the email. Please save the copy for future reference. The application can be found in the "Review &amp; Process" tab on &lt;a href="https://digital-journeys.apps.silver.devops.gov.bc.ca/form" rel="noopener noreferrer nofollow" target="_blank"&gt;Digital Journeys&lt;/a&gt;.
