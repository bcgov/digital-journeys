          <text></text>
        </inputEntry>
        <inputEntry id="UnaryTests_1inm09j">
          <text>"yesHoweverOnlyIWillBeTakingParentalLeave","yesBothMyPartnerAndIWillBeTakingParentalLeave"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_0k64coc">
          <text></text>
        </inputEntry>
        <inputEntry id="UnaryTests_178usk8">
          <text></text>
        </inputEntry>
        <outputEntry id="LiteralExpression_1er35u9">
