      <rule id="DecisionRule_1r01meb">
        <inputEntry id="UnaryTests_0jln6ye">
          <text></text>
        </inputEntry>
        <inputEntry id="UnaryTests_0n3aix7">
          <text></text>
        </inputEntry>
        <outputEntry id="LiteralExpression_10xwysn">
          <text></text>
