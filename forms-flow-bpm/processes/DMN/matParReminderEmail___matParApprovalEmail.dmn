      <rule id="DecisionRule_1886d85">
        <description>defer, parental</description>
        <inputEntry id="UnaryTests_0z1urec">
          <text>"APPROVED"</text>
        </inputEntry>
        <inputEntry id="UnaryTests_1pnsvbb">
          <text></text>
        </inputEntry>
